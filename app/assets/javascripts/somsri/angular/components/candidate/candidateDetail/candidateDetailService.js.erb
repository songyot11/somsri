(function() {
  'use strict';

  angular.module('somsri.somsri.candidate_detail.service', [])
  .service('candidateDetailService', ['$http', function($http) {

  this.getById = function(id){
    return $http.get('/candidates/' + id);
  }
  this.editStar = function(id, check){
    return $http({
      url: '/candidates/'+ id + '/edit_star/',
      method: "POST",
      params: {bool: check}
    })
  }

  }]);
})();


